{
    "name": "todo",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "nodemon ./bin/www",
        "start-dev": "NODE_ENV_HEADER=local nodemon ./bin/www",
        "test": "NODE_ENV=test mocha --timeout 360000 ./test/**/**.test.js --exit",
        "test-dev": "NODE_ENV=test mocha --timeout 360000 ./test/**/**.test.js --exit",
        "coverage": "NODE_ENV=test nyc --reporter=html mocha --timeout 600000 ./test/**/**.test.js --exit",
        "db-migrations": "node_modules/.bin/sequelize db:migrate --config=config/config.json",
        "test-migrations": "node_modules/.bin/sequelize db:migrate --config=config/config.json --env test",
        "seed": "node_modules/.bin/sequelize db:seed:all --config=config/config.json",
        "seedOne": "node_modules/.bin/sequelize db:seed --seed --config=config/config.json"
    },
    "dependencies": {
        "@microsoft/microsoft-graph-client": "^1.0.0",
        "async": "^2.6.1",
        "aws-sdk": "2.368.0",
        "bcrypt": "^3.0.8",
        "bcrypt-promise": "^2.0.0",
        "body-parser": "^1.18.3",
        "cloudinary": "^1.14.0",
        "cookie-parser": "~1.4.3",
        "cors": "^2.8.5",
        "cron": "^1.5.0",
        "crypto": "^1.0.1",
        "debug": "~2.6.9",
        "dotenv": "^5.0.0",
        "emailjs-imap-client": "^3.0.7",
        "express": "^4.16.4",
        "faker": "^4.1.0",
        "fast-csv": "^2.4.1",
        "gm": "^1.23.1",
        "googleapis": "^27.0.0",
        "html-to-text": "^4.0.0",
        "imap": "^0.8.19",
        "jade": "~1.11.0",
        "js-base64": "^2.5.1",
        "jsonwebtoken": "^8.5.1",
        "mimemessage": "^1.0.5",
        "moment": "^2.24.0",
        "morgan": "^1.9.1",
        "multer": "^1.4.1",
        "multer-s3": "^2.9.0",
        "multer-storage-cloudinary": "^2.2.1",
        "mysql2": "^1.6.4",
        "nodemailer": "^4.7.0",
        "nodemon": "^1.18.7",
        "parse-error": "^0.2.0",
        "passport": "^0.4.0",
        "passport-jwt": "^4.0.0",
        "path": "^0.12.7",
        "request": "^2.88.0",
        "s3-zip": "^2.0.1",
        "sequelize": "^4.41.2",
        "sequelize-cli": "^5.3.0",
        "serve-favicon": "~2.4.5",
        "simple-oauth2": "^2.2.1",
        "socket.io": "^2.1.1",
        "stream": "0.0.2",
        "validator": "^9.4.1",
        "yarn": "^1.15.2"
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "chai-http": "^4.2.0",
        "mocha": "^5.2.0",
        "nyc": "^12.0.2"
    }
}
